{
  "name": "@mdsol/onex-design",
  "version": "0.4.1",
  "author": "JT <jathompson@mdsol.com>",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mdsol/onex-design.git"
  },
  "main": "./dist/index.compat.js",
  "module": "./dist/index.umd.js",
  "publishConfig": {
    "registry": "https://mdsol.jfrog.io/artifactory/api/npm/onex_design-npm-prod-local/"
  },
  "engines": {
    "node": ">=16.14.2",
    "npm": ">=8.5.0",
    "yarn": ">= 3.2.0"
  },
  "scripts": {
    "dev": "webpack serve --mode development",
    "storybook": "start-storybook -p 6006",
    "build": "webpack --config webpack.config.pkg.js --mode production --json > stats.json && babel dist/*.umd.js -d dist --out-file-extension .compat.js",
    "build:size": "webpack-bundle-analyzer stats.json",
    "build:css": "sass --style=compressed ./src/scss/platform.scss dist/platform.css",
    "build:css:tp": "node src/utils/themeprovider-css.js",
    "build:sb": "build-storybook",
    "lint": "eslint ./src --cache && echo \"No linting errors\"",
    "lint:fix": "eslint ./src --fix",
    "sc-css": "node src/utils/styled-css.js base",
    "sc-jsx": "node src/utils/styled-jsx/index.js",
    "deploy:sb": "storybook-to-ghpages",
    "utils:bl": "browserslist@latest --update-db",
    "utils:sb-browsers": "node src/utils/sb-browsers.js",
    "test": "jest",
    "test:styled": "STYLED=true jest"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "prop-types": "^15.8.1",
    "react-bootstrap": "^2.4.0",
    "react-date-range": "^1.4.0",
    "react-dropzone": "^14.2.1",
    "react-select": "^5.3.2",
    "react-table": "^7.8.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-private-methods": "^7.17.12",
    "@babel/plugin-proposal-private-property-in-object": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-docs": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "6.5.9",
    "@storybook/storybook-deployer": "^2.8.11",
    "@storybook/ui": "6.5.9",
    "babel-loader": "^8.2.5",
    "browserslist": "^4.20.4",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.23.2",
    "css-loader": "^6.7.1",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-webpack-plugin": "^3.1.1",
    "fs-extra": "^10.1.0",
    "got": "^12.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jest-image-snapshot": "^4.5.1",
    "jsdom": "^19.0.0",
    "prettier": "^2.7.1",
    "puppeteer": "^13.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-is": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.52.3",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ]
  }
}
