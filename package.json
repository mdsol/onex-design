{
  "name": "@mdsol/onex-design",
  "version": "0.1.3",
  "author": "JT <jathompson@mdsol.com>",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mdsol/onex-design.git"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": "./dist/index.umd.js",
    "./dist/*": "./dist/*"
  },
  "publishConfig": {
    "registry": "https://mdsol.jfrog.io/artifactory/api/npm/onex_design-npm-prod-local/"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 3.2.0"
  },
  "packageManager": "yarn@3.2.0-rc.12",
  "scripts": {
    "dev": "webpack serve --mode development",
    "storybook": "start-storybook -p 6006",
    "build": "webpack --config webpack.config.pkg.js --mode production",
    "build:css": "sass --style=compressed ./src/scss/platform.scss dist/platform.css",
    "build:sb": "build-storybook",
    "lint": "eslint ./src --cache && echo \"No linting errors\"",
    "lint:fix": "eslint ./src --fix",
    "sc-css": "node src/utils/styled-css.js base",
    "sc-jsx": "node src/utils/styled-jsx/index.js",
    "deploy:sb": "storybook-to-ghpages",
    "utils:bl": "browserslist@latest --update-db",
    "utils:sb-browsers": "node src/utils/sb-browsers.js",
    "test": "jest",
    "test:styled": "STYLED=true jest"
  },
  "dependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.16.11",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@mdx-js/react": "^1.6.22",
    "@mui/icons-material": "^5.5.0",
    "@mui/material": "^5.5.0",
    "@popperjs/core": "^2.11.2",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-docs": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/ui": "6.3.12",
    "babel-loader": "^8.2.3",
    "bootstrap": "^5.1.3",
    "browserslist": "^4.19.1",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.21.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-webpack-plugin": "^3.1.1",
    "fs-extra": "^10.0.0",
    "got": "^11.8.3",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jest-image-snapshot": "^4.5.1",
    "jsdom": "^18.1.1",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "puppeteer": "^13.3.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-bootstrap-icons": "^1.7.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-is": "^17.0.2",
    "react-select": "^5.2.2",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.3",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ]
  }
}
