{
  "name": "@mdsol/onex-design",
  "version": "0.1.0",
  "private": true,
  "author": "JT <jathompson@mdsol.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdsol/onex-design.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 3.1.0"
  },
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "babel src/components --out-dir dist --copy-files",
    "build:webpack": "webpack --mode production",
    "build:theme": "sass ./src/scss/theme.scss dist/theme.css",
    "build:sb-browsers": "node src/tools/sb-browsers.js",
    "build:sb-icons": "node src/tools/sb-icons.js",
    "build:storybook": "build-storybook",
    "package": "webpack --config webpack.config.pkg.js --mode production",
    "lint": "eslint ./src --cache && echo \"No linting errors\"",
    "lint:fix": "eslint ./src --fix",
    "bs-css": "node src/tools/bs-css.js",
    "storybook": "start-storybook -p 6006",
    "deploy:sb": "storybook-to-ghpages",
    "update:bl": "browserslist@latest --update-db"
  },
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@mdx-js/react": "^1.6.22",
    "@popperjs/core": "^2.10.2",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-docs": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/ui": "6.3.12",
    "babel-loader": "^8.2.3",
    "bootstrap": "^5.1.3",
    "browserslist": "^4.18.1",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.19.1",
    "css-loader": "^6.5.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^18.1.0",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "query-ast": "^1.0.4",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-bootstrap-icons": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-is": "^17.0.2",
    "react-select": "^5.2.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "sass-to-string": "^1.6.0",
    "scss-parser": "^1.0.5",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.3",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
